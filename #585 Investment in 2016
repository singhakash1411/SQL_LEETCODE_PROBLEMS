# Write your MySQL query statement below
select 
Round(sum(tiv_2016), 2)AS tiv_2016
from Insurance 
where tiv_2015 in(
    select tiv_2015
    from Insurance
    Group By tiv_2015
    Having count(*)>1
)
AND (lat, lon) IN (
    select lat, lon 
    from Insurance 
    Group By lat,lon
    Having count(*)=1
);
